@model MVCData.Models.Person

<p>
    @ViewBag.Person
</p>

<table class="table table-bordered">
    <tr>
        <td class="success"><b>Name</b></td>
        <td class="active">@Model.Name</td>
    </tr>
    <tr>
        <td class="active"><b>Cellphone</b></td>
        <td>@Model.Cellphone</td>
    </tr>
    <tr>
        <td class="active"><b>City</b></td>
        <td>@Model.City</td>
    </tr>
    <tr>
        <td class="active"><b>Country</b></td>
        <td>@Model.Country</td>
    </tr>
</table>

